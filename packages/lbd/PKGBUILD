pkgname=lbd
pkgver=0.4
pkgrel=1
epoch=1
groups=('archassault' 'archassault-reconnaissance')
pkgdesc="Load Balancing detector"
url='http://ge.mine.nu/code/lbd'
arch=('any')
license=('GPL2')
depends=('bash')
source=(https://raw.githubusercontent.com/craig/ge.mine.nu/master/lbd/lbd.sh)
